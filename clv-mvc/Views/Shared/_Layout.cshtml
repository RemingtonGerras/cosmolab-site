<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="">
    <meta name="author" content="Prayaas Aggarwal, Vera Gluscevic, Mark Otto, Jacob Thornton, and Bootstrap contributors">

    <title>@ViewData["Title"] - USC CosmoLab</title>

    <link rel="canonical" href="link-to-homepage-here">

    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

    <link rel="apple-touch-icon" href="https://getbootstrap.com/docs/4.4/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
    <link rel="icon" href="https://getbootstrap.com/docs/4.4/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
    <link rel="icon" href="https://getbootstrap.com/docs/4.4/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
    <link rel="manifest" href="https://getbootstrap.com/docs/4.4/assets/img/favicons/manifest.json">
    <link rel="mask-icon" href="https://getbootstrap.com/docs/4.4/assets/img/favicons/safari-pinned-tab.svg" color="#563d7c">
    <link rel="icon" href="https://getbootstrap.com/docs/4.4/assets/img/favicons/favicon.ico">
    <meta name="msapplication-config" content="https://getbootstrap.com/docs/4.4/assets/img/favicons/browserconfig.xml">
    <meta name="theme-color" content="#563d7c">
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="~/css/hamburgers.css">
</head>
<body class="bg">
    <div class="container-fluid">
        <div class="row">
            <nav style=" background-color:#990000;" class="col-0 col-md-1 col-lg-2 d-sm-none d-md-block sidebar">
                <div class="sidebar-sticky">
                    <div class="nav flex-column">
                        <button class="hamburger hamburger--collapse is-active" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                        <div class="nav-item">
                            <a class="nav-link active row" asp-area="" asp-controller="Home" asp-action="Index">
                                <div style="text-align: center;" class="col-12 col-lg-3 d-inline-block float-left px-1">
                                    <span data-feather="home"></span>
                                </div>
                                <div style="text-align: left;" class="col-0 col-lg-9 d-none d-lg-inline-block px-0">Home</div>
                            </a>
                            @*<a class="nav-link active" asp-area="" asp-controller="Home" asp-action="Index">
                                <span data-feather="home"></span>
                                Home <span class="sr-only">(current)</span>
                            </a>*@
                        </div>
                        <div class="nav-item">
                            <a class="nav-link row" asp-area="" asp-controller="Home" asp-action="Visualization">
                                <div style="text-align: center;" class="col-12 col-lg-3 d-inline-block float-left px-1">
                                    <span data-feather="layers"></span>
                                </div>
                                <div style="text-align: left;" class="col-0 col-lg-9 d-none d-lg-inline-block px-0">Visualization</div>
                            </a>
                        </div>
                        <div class="nav-item">
                            <a class="nav-link row" href="research.html">
                                <div style="text-align: center;" class="col-12 col-lg-3 d-inline-block float-left px-1">
                                    <span data-feather="file"></span>
                                </div>
                                <div style="text-align: left;" class="col-0 col-lg-9 d-none d-lg-inline-block px-0">Research</div>
                            </a>
                        </div>
                        <div class="nav-item">
                            <a class="nav-link row" href="people.html">
                                <div style="text-align: center;" class="col-12 col-lg-3 d-inline-block float-left px-1">
                                    <span data-feather="users"></span>
                                </div>
                                <div style="text-align: left;" class="col-0 col-lg-9 d-none d-lg-inline-block px-0">People</div>
                            </a>
                        </div>
                        <div class="nav-item">
                            <a class="nav-link row" href="#">
                                <div style="text-align: center;" class="col-12 col-lg-3 d-inline-block float-left px-1">
                                    <span data-feather="book"></span>
                                </div>
                                <div style="text-align: left;" class="col-0 col-lg-9 d-none d-lg-inline-block px-0">Publications</div>
                            </a>
                        </div>
                        <div class="nav-item">
                            <a class="nav-link row" href="#">
                                <div style="text-align: center;" class="col-12 col-lg-3 d-inline-block float-left px-1">
                                    <span data-feather="book-open"></span>
                                </div>
                                <div style="text-align: left;" class="col-0 col-lg-9 d-none d-lg-inline-block px-0">News</div>
                            </a>
                        </div>
                        <div class="nav-item">
                            <a class="nav-link row" href="#">
                                <div style="text-align: center;" class="col-12 col-lg-3 d-inline-block float-left px-1">
                                    <span data-feather="info"></span>
                                </div>
                                <div style="text-align: left;" class="col-0 col-lg-9 d-none d-lg-inline-block px-0">Openings</div>
                            </a>
                        </div>
                        <div class="nav-item">
                            <a class="nav-link row" href="#">
                                <div style="text-align: center;" class="col-12 col-lg-3 d-inline-block float-left px-1">
                                    <span data-feather="message-square"></span>
                                </div>
                                <div style="text-align: left;" class="col-0 col-lg-9 d-none d-lg-inline-block px-0">Contact</div>
                            </a>
                        </div>
                    </div>
                </div>
            </nav>

            <main role="main" style="padding-top: 0; height: 100%;" class="col-12 col-md-11 offset-md-1 col-lg-10 offset-lg-2 px-4 main">
                @RenderBody()
            </main>
        </div>

    </div>


    <script src="https://code.jquery.com/jquery-3.4.1.min.js"
            integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
            crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script>window.jQuery || document.write('<script src="https://getbootstrap.com/docs/4.4/assets/js/vendor/jquery.slim.min.js"><\/script>')</script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/feather-icons/4.9.0/feather.min.js"></script>
    <script src="https://getbootstrap.com/docs/4.4/dist/js/bootstrap.bundle.min.js" integrity="sha384-6khuMg9gaYr5AxOqhkVIODVIvm9ynTT5J4V1cfthmT+emCG6yVmEZsRHdxlotUnm" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/highcharts.src.js"></script>
    <script src="~/js/themes/dark-unica.js"></script>
    <script>

        jQuery.ajax({
            type: "POST",
            url: "/Home/NewChart",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (chData) {

                var myChart = Highcharts.chart('chart-container', {
                    chart: {
                        type: 'line'
                    },
                    title: {
                        text: 'The CMB anisotropy spectra'
                    },
                    yAxis: {
                        title: {
                            text: 'that value'
                        }
                    },
                    series: [
                        {
                            name: 'TT',
                            data: chData[0]
                        },
                        {
                            name: 'EE',
                            data: chData[1]
                        },
                        {
                            name: 'TE',
                            data: chData[2]
                        },
                        {
                            name: 'BB',
                            data: chData[3]
                        },
                        {
                            name: 'phiphi',
                            data: chData[4]
                        },
                        {
                            name: 'TPhi',
                            data: chData[5]
                        },
                        {
                            name: 'EPhi',
                            data: chData[6]
                        }
                    ]
                });
            }
        });
    </script>
    @*<script>
        </script>*@
    @RenderSection("Scripts", required: false)
</body>
</html>
